@import "styles/iconfont.wxss";

/* 小程序不支持通配符* */

page,
view,
text,
swiper,
swiper-item,
image,
navgigator {
    padding: 0;
    margin: 0;
    box-sizing: border-box;
}

page {
    /* 主题颜色，通过变量来实现，原生wxss */
    --themeColor: #eb4450;
    /* 定义统一的字体大小14px=28rpx */
    font-size: 28rpx;
}

image {
    width: 100%;
}